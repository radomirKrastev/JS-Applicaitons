{{>navbar}}
<div class="profile col-md-6 text-center col-lg">
    <img class="profile-img" src="./images/user.png" />
    <div class="profile-info">
        <p>Username: <small>{{username}}</small></p>
        <p class="infoType">Wished {{treksCount}} treks =)</p>
        {{#if totalUserTreks}}
        {{#each totalUserTreks}}
        {{>userTrek}}
        {{/each}}
        {{else}}
        <p>No treks</p>
        {{/if}}
    </div>
</div>
{{>footer}}